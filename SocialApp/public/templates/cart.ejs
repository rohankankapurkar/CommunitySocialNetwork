<div class="jumbotron">
    <h3><center> Accept or reject member </center></h3>
  </div>




<div class="div-align" ng-controller="cartController">
	<div>
		<table class="table table-bordered table-hover table-active">
			<tr>
				<th>Firstnme</th>
				<th>Lastname</th>
				<th>Email</th>
				<th>Address</th>
				<th>Role</th>

				<th>Community</th>
				<th>Approve</th>
                <th>Reject</th>
                
			</tr>
			<tr ng-repeat="product in allproducts">
				<td>{{product.firstname}}</td>
				<td>{{product.lastname}}</td>
				<td>{{product.email}}</td>
				<td>{{product.address}}</td>
				<td>{{product.role}}</td>
                <td>{{product.community}}</td>
                
				<td><button name="Approve" ng-click="purchase(product.email)" type="submit"
											class="btn btn-success btn-round ">Approve</button></td>
                
                <td><button name="Reject" ng-click="deleteFromCart(product.email)" type="submit"
											class="btn btn-danger btn-round ">Reject</button></td>
            </tr>
		</table>

	</div>
    

    

    
 
    
    
    
    
</div>
